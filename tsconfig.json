{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.jsx",
    "src/pages/RegEmail.jsx",
    "src/pages/RegCpf.jsx",
    "src/pages/Register/RegCelular.jsx",
    "src/utils/useLocalStorage.jsx",
    "src/utils/useLocalStorage.jsx",
    "src/components/Corpo/index.jsx",
    "src/pages/register/name.jsx",
    "src/pages/password.jsx",
    "src/pages/componente.jsx",
    "src/pages/confirmation.jsx",
    "src/pages/confirm.jsx",
    "src/pages/business-register/address.jsx",
    "src/pages/business-register/postal.jsx",
    "src/pages/business-register/companyname.jsx",
    "src/pages/business-register/cnpj.jsx",
    "src/pages/register/birth.jsx",
    "src/pages/register/confirm.jsx",
    "src/pages/business-register/mean.jsx",
    "src/components/checkbox/index.jsx",
    "src/components/checkbox/index.jsx",
    "src/pages/business-register/category.jsx",
    "src/pages/404.js"
  ],
  "exclude": ["node_modules"]
}
